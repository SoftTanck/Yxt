<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/course_linear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00AAE9"
    android:orientation="vertical"
    tools:context="com.softtanck.framework.activity.MySchoolBagAndSignUpActivity">

    <include
        android:id="@+id/comm_titleView"
        layout="@layout/view_common_title"/>

    <com.softtanck.framework.pulltorefresh.PullToRefreshExpandableListView
        xmlns:ptr="http://schemas.android.com/apk/res-auto"
        android:id="@+id/pull_refresh_expandable_list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:divider="@null"
        android:background="@mipmap/ic_v4_theme_default_main_bg"
        ptr:ptrHeaderBackground="#00000000"
        ptr:ptrHeaderTextColor="@android:color/black"
        ptr:ptrMode="pullDownFromTop" />
